      <% @occupations.each do |occupation| %>
        <li>
          <%= render "shared/superuser_countryadmin_notifications", model_name: occupation %>
          <%= occupation.jobfamily.job_family %>
          
          <span class="r-float">
            <%= link_to "edit", edit_reserved_occupation_path(occupation) if current_user.superuser? %>
            <% if current_user.superuser? || current_user.administrator?(@country.country) %>    
              | <%= link_to "del", occupation, method: :delete,
                             data: { confirm: "Delete '#{occupation.jobfamily.job_family}' from the reserved occupations list?  Sure?" } %>
            <% end %>
          </span>
          <% end %>
        </li>
      <% end %>
