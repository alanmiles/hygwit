<% provide(:title, 'New Currency') %>
<h1>New Currency</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@currency) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'fields', f: f %>
      <%= f.submit "Create", class: "btn btn-primary" %>
      <%= link_to "Back", currencies_path, class: "btn" %>
    <% end %>
  </div>
</div>
