      <% @leaving_reasons.each do |reason| %>
        <li>
          <%= render "shared/superuser_notifications", model_name: reason %>
          <%= reason.reason %>
          <% if reason.full_benefits? %>
            | Full leaving benefits
          <% end %>
          | <%= link_to "change", edit_leaving_reason_path(reason) %>
          <%= render "shared/admin_deletes", model_name: quality %>
        </li>
      <% end %>
