         <% if current_user.superuser? %>
           <% if @country.complete? %>
             <%= superuser_complete %>
           <% else %>
             <%= superuser_incomplete %>
           <% end %>
         <% elsif current_user.administrator?(@country.country) %>
           <% if @country.complete? %>
             <%= admin_complete %>
           <% else %>
             <%= admin_incomplete %>
           <% end %>
         <% else %>
           <%= non_admin %>
         <% end %>
